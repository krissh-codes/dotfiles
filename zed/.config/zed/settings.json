{
    "assistant": {
        "dock": "left",
        "default_model": {
            "provider": "zed.dev",
            "model": ""
        },
        "version": "2"
    },
    "icon_theme": "Material Icon Theme",
    "features": {
        "edit_prediction_provider": "zed"
    },
    "outline_panel": {
        "dock": "right",
        "auto_fold_dirs": true
    },
    "project_panel": {
        "dock": "right"
    },
    "theme": {
        "mode": "system",
        "light": "Catppuccin Latte (Blur)",
        "dark": "Catppuccin Mocha (Blue Blur+)"
    },
    "tab_bar": {
        "show_nav_history_buttons": false,
        "show": true
    },
    "ui_font_family": ".SystemUIFont",
    "ui_font_size": 15.0,
    "buffer_font_family": "MesloLGM Nerd Font",
    "buffer_font_size": 12.0,
    "toolbar": {
        "breadcrumbs": false,
        "quick_actions": false
    },
    "base_keymap": "JetBrains",
    "buffer_font_features": {
        "liga": true
    },
    "confirm_quit": true,
    "tab_size": 4,
    "remove_trailing_whitespace_on_save": false,
    "use_on_type_format": false,
    "format_on_save": "off",
    "preferred_line_length": 180,
    "terminal": {
        "font_family": "MesloLGM Nerd Font",
        "copy_on_select": true,
        "shell": {
            "program": "/opt/homebrew/bin/fish"
        },
        "font_size": 11.5,
        "toolbar": {
            "title": false
        }
    },
    "soft_wrap": "editor_width"
}
