{
    "features": {
        "inline_completion_provider": "none"
    },
    "outline_panel": {
        "dock": "right",
        "auto_fold_dirs": true
    },
    "project_panel": {
        "dock": "left"
    },
    "theme": {
        "mode": "system",
        "light": "Catppuccin Latte (Blur)",
        "dark": "Catppuccin Mocha (Blur)"
    },
    "tab_bar": {
        "show_nav_history_buttons": false,
        "show": true
    },
    "ui_font_family": "Inter Variable",
    "ui_font_size": 14.5,
    "buffer_font_family": "FiraCode Nerd Font",
    "buffer_font_size": 12,
    "toolbar": {
        "breadcrumbs": false,
        "quick_actions": false
    },
    "base_keymap": "JetBrains",
    "buffer_font_features": {
        "liga": true
    },
    "confirm_quit": true,
    "tab_size": 4,
    "remove_trailing_whitespace_on_save": false,
    "use_on_type_format": false,
    "format_on_save": "off",
    "preferred_line_length": 180,
    "terminal": {
        "font_family": "FiraCode Nerd Font",
        "copy_on_select": true,
        "shell": {
            "program": "/opt/homebrew/bin/fish"
        },
        "font_size": 11.5,
        "toolbar": {
            "title": false
        }
    },
    "experimental.theme_overrides": {
        "border": "#9494941a"
    },
    "soft_wrap": "editor_width"
}
